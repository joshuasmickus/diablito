    "react-dom": "~0.14.3"
  },
  "devDependencies": {
    "babel-cli": "~6.6.5",
    "babel-core": "~6.7.4",
    "babel-eslint": "~5.0.0",
    "chai": "~3.2.0",
    "eslint": "~2.4.0",
    "gulp": "~3.9.1",
    "jsdom": "~6.5.1",
    "mocha": "~2.4.5",
